{
  "expo": {
    "name": "maps-app",
    "slug": "maps-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "config": {
        "googleMapsApiKey": "AIzaSyCEGhCDVK4IMOhLTEeCTXA7wZ45LpeAOrA"
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.mtorres93.mapsapp"
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAyMjWpP0rBdPOeXYPQx_LxUWTC6K8lguU"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mtorres93.mapsapp",
      "permissions": []
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "5d42c95a-94fa-4643-82a6-54adb97fd2fa"
      }
    }
  }
}
